{
  "name": "School Finder",
  "image": "python:3.11-slim",
  "postCreateCommand": "pip install -r requirements.txt",
  "postStartCommand": "uvicorn app.main:app --host 0.0.0.0 --port 8000 &",
  "forwardPorts": [8000],
  "portsAttributes": {
    "8000": {
      "label": "School Finder App",
      "onAutoForward": "openBrowser"
    }
  }
}
